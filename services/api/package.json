    "@supabase/supabase-js": "^2.91.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/cors": "^2.8.19",
    "bullmq": "^5.66.7",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "ioredis": "^5.9.2",
    "jimp": "^1.1.4",
    "jsonwebtoken": "^9.0.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
